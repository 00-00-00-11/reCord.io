{
    "help": {
      "access": 0,
      "scope": "none",
      "desc": "Shows this list or displays command usage.",
      "usage": "help <blank / command name> ",
      "group": "general"
    },
    "ping": {
      "access": 1,
      "scope": "none",
      "desc": "Call this for an amazing response.",
      "usage": "ping",
      "group": "general"
    },


    "audio": {
      "access": 2,
      "scope": "none",
      "desc": "Joins voice and plays an MP3 File specified.",
      "usage": "audio <audiofilename>",
      "group": "playback"
    },

    "simulate": {
      "access": 3,
      "scope": "none",
      "desc": "Simulates a specified mentioned user (or the whole text channel) by using Markov Chains.",
      "usage": "simulate <blank (simulates whole text channel) / @user>",
      "group": "general"
    },

    "stopsimulation": {
      "access": 3,
      "scope": "none",
      "desc": "Stops the simulation in the current text channel (if any).",
      "usage": "stopsimulation",
      "group": "general"
    },


    "q": {
      "access": 2,
      "scope": {
        "2": "links",
        "3": "search",
        "4": "playlists",
        "5": "unlimited"
      },
      "desc": "[Alias for queue] Streams audio from YouTube. Accepts either links or search queries.",
      "usage": "q <link / search query>",
      "group": "playback"

    },

    "queue": {
      "access": 2,
      "scope": {
        "2": "links",
        "3": "search",
        "4": "playlists",
        "5": "unlimited"
      },
      "desc": "Streams audio from YouTube. Accepts either links or search queries.",
      "usage": "queue <link / search query>",
      "group": "playback"

    },

    "play": {
      "access": 2,
      "scope": {
        "2": "links",
        "3": "search",
        "4": "playlists",
        "5": "unlimited"
      },
      "desc": "[Alias for queue] Streams audio from YouTube. Accepts either links or search queries.",
      "usage": "play <link / search query>",
      "group": "playback"
    },

    "volume": {
      "access": 3,
      "scope": {
        "3": "50",
        "4": "100",
        "5": "200"
      },
      "desc": "Change the volume for the active queue.",
      "usage": "volume <0-200>",
      "group": "playback"
    },
    "skip": {
      "access": 1,
      "scope": {
        "1": "vote",
        "2": "self",
        "5": "instant"
      },
      "desc": "skips the currently playing song. (Only for active queue)",
      "usage": "skip",
      "group": "playback"
    },

    "roll": {
      "access": 0,
      "scope": "none",
      "desc": "Returns a random number from 1-100",
      "usage": "roll",
      "group": "general"
    },
    "imgur": {
      "access": 3,
      "scope": "none",
      "desc": "Search imgur or leave empty to return a completely random image.",
      "usage": "imgur <query / blank>",
      "group": "general"
    },

    "joinvoice": {
      "access": 2,
      "scope": "none",
      "desc": "Joins voice channel.",
      "usage": "joinvoice",
      "group": "playback"
    },
    "leavevoice": {
      "access": 2,
      "scope": "none",
      "desc": "Leaves voice channel.",
      "usage": "leavevoice",
      "group": "playback"
    }

}
